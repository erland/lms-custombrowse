<?xml version="1.0" encoding="utf-8"?>
<custombrowse>
	<requireplugins>AlbumReview</requireplugins>
	<mix>
		<mixname>AlbumReview</mixname>
		<mixcategory>album</mixcategory>
		<mixtype>mode</mixtype>
		<mixchecktype>function</mixchecktype>
                <mixcheckdata>Plugins::CustomBrowse::Plugin::albumreviewMixable</mixcheckdata>
		<mixdata>Plugins::AlbumReview::Plugin|album_id={itemid}</mixdata>
		<mixurl>plugins/AlbumReview/index.html?&amp;album_id={itemid}</mixurl>
		<miximage>html/images/albumreview_mix.gif</miximage>
	</mix>
</custombrowse>
