<?xml version="1.0" encoding="utf-8"?>
<custombrowse>
	<requireplugins>Biography</requireplugins>
	<mix>
		<mixname>Biography</mixname>
		<mixcategory>artist</mixcategory>
		<mixtype>mode</mixtype>
		<mixchecktype>function</mixchecktype>
                <mixcheckdata>Plugins::CustomBrowse::Plugin::biographyMixable</mixcheckdata>
		<mixdata>Plugins::Biography::Plugin|artist_id={itemid}</mixdata>
		<mixurl>plugins/Biography/index.html?&amp;artist_id={itemid}</mixurl>
		<miximage>html/images/biography_mix.gif</miximage>
	</mix>
</custombrowse>
