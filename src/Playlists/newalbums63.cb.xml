<?xml version="1.0" encoding="utf-8"?>
<custombrowse>
	<maxslimserverversion>6.4</maxslimserverversion>
	<menu>
	        <id>newalbums</id>
	        <menuname>New Albums</menuname>
	        <menu>
	                <id>album</id>
			<menuname>Songs</menuname>
			<itemtype>album</itemtype>
			<itemformat>album</itemformat>
	                <menutype>sql</menutype>
	                <menudata>
				select albums.id,albums.title from tracks,albums 
				where 
					tracks.audio=1 and 
					albums.id=tracks.album 
				group by albums.id 
				order by max(tracks.age) desc
			</menudata>
			<menu>
				<id>track</id>
				<itemtype>track</itemtype>
				<itemformat>track</itemformat>
				<playtype>all</playtype>
				<menutype>sql</menutype>
				<menudata>
					select tracks.id,tracks.title from tracks 
					where 
						tracks.audio=1 and 
						tracks.album={album} 
					order by tracks.age desc
				</menudata>
				<menu>
					<id>trackdetails</id>
					<menutype>trackdetails</menutype>
					<menudata>track</menudata>
				</menu>
			</menu>
		</menu>
	</menu>
</custombrowse>
