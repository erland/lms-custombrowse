<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<section id="nowplaying">
		<subsection id="custombrowse_mix">
			<name>CB Mixes:</name>
			<weight>10</weight>
			<command id="songmix">
				<defaultenabled>0</defaultenabled>
				<requireplugins>CustomBrowse</requireplugins>
				<weight>10</weight>
				<name>Song</name>
				<icon>html/images/mixbutton.png</icon>
				<url>plugins/CustomBrowse/custombrowse_mixlist.html?contexttype=track</url>
				<type>content</type>
				<parameters>
					<id>contextid</id>
					<title>contextname</title>
					<player>player</player>
				</parameters>
			</command>
			<command id="albummix">
				<requireplugins>CustomBrowse</requireplugins>
				<weight>20</weight>
				<name>Album</name>
				<icon>html/images/mixbutton.png</icon>
				<url>plugins/CustomBrowse/custombrowse_mixlist.html?contexttype=album</url>
				<type>content</type>
				<parameters>
					<album_id>contextid</album_id>
					<album>contextname</album>
					<player>player</player>
				</parameters>
			</command>
			<command id="artistmix">
				<requireplugins>CustomBrowse</requireplugins>
				<weight>30</weight>
				<name>Artist</name>
				<icon>html/images/mixbutton.png</icon>
				<url>plugins/CustomBrowse/custombrowse_mixlist.html?contexttype=artist</url>
				<type>content</type>
				<parameters>
					<artist_id>contextid</artist_id>
					<artist>contextname</artist>
					<player>player</player>
				</parameters>
			</command>
		</subsection>
	</section>
</configuration>
