<?xml version="1.0" encoding="utf-8"?>
<custombrowse>
	<requireplugins>LMA</requireplugins>
	<template>
		<downloadidentifier>91</downloadidentifier>
		<name>Live Music Archive</name>
		<description>Menu that puts the standard Live Music Archive menu below Custom Browse menu</description>
		<parameter type="text" id="menuname" name="Menu name" value="Live Music Archive"/>
		<parameter type="text" id="menugroup" name="Menu group" value=""/>
		<parameter type="text" id="menuorder" name="Menu sort order (1-100)" value="50"/>
		<parameter type="text" id="includedclients" name="Only include on these players (empty = all)" value=""/>
		<parameter type="text" id="excludedclients" name="Exclude from these players" value=""/>
	</template>
</custombrowse>

