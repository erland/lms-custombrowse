<?xml version="1.0" encoding="utf-8"?>
<custombrowse>
	 <minpluginversion>1.19</minpluginversion>
	<template>
		<downloadidentifier>93</downloadidentifier>
		<name>uPNP Device</name>
		<description>Menu that puts the standard uPNP device menu below Custom Browse menu, for example Rhapsody</description>
		<parameter type="text" id="menuname" name="Menu name" value="uPNP device"/>
		<parameter type="text" id="menugroup" name="Menu group" value=""/>
		<parameter type="text" id="menuorder" name="Menu sort order (1-100)" value="50"/>
		<parameter type="functionsinglelist" id="deviceudn" name="uPNP device" value="0" data="Plugins::CustomBrowse::Plugin,getAvailableuPNPDevices"/>
		<parameter type="text" id="includedclients" name="Only include on these players (empty = all)" value=""/>
		<parameter type="text" id="excludedclients" name="Exclude from these players" value=""/>
	</template>
</custombrowse>

