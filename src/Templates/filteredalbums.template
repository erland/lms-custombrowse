                        </menudata>
                        <menu>
                                <id>track</id>
                                <itemtype>track</itemtype>
                                <itemformat>track</itemformat>
				[%- IF playalltracks %]
				<playtype>all</playtype>
				[%- END %]
                                <menutype>sql</menutype>
                                <menudata>
					select tracks.id,tracks.title from tracks[% IF includedgenres %],genre_track,genres[% END %][% IF includedcomment %],comments[% END %][% IF library or activelibrary %],multilibrary_track[% END %][% IF neverplayed OR minrating OR nullrating OR exactrating %],track_statistics[% END %]
					where
						[%- IF library or activelibrary %]
						tracks.id=multilibrary_track.track and
						multilibrary_track.library=[% IF activelibrary %]{clientproperty:plugin.multilibrary:activelibraryno}[% ELSE %][% library %][% END %] and
						[%- END %]
						[%- IF neverplayed OR minrating OR nullrating OR exactrating %]
						tracks.url=track_statistics.url and
						[%- END %]
						[%- IF neverplayed %]
						ifnull(track_statistics.playcount,0)=0 and
						[%- END %]
						[%- IF minrating OR nullrating %]
						([%- IF nullrating -%]ifnull(track_statistics.rating,0)=0 or [%- END -%]track_statistics.rating&gt;=[% minrating %]) and
						[%- END %]
						[%- IF exactrating %]
						track_statistics.rating&gt;=[% exactrating - 10 %] and track_statistics.rating&lt;=[% exactrating + 9 %] and
						[%- END %]
						[%- IF lossless %]
						tracks.lossless=1 and
						[%- END %]
						[%- IF includedgenres %]
						tracks.id=genre_track.track
						and genre_track.genre=genres.id and
						genres.name in ([% includedgenres %]) and
						[%- END %]
						[%- IF includedcomment %]
						tracks.id=comments.track and
						[%- IF containsincludedcomment %]
						comments.value like '%[% includedcomment %]%'
						[% ELSE %]
						comments.value='[% includedcomment %]'
						[%- END %]
						and
						[%- END %]
						tracks.album={album}
						[%- IF excludedgenres %]
						and not exists(select * from genre_track,genres 
							where 
								genre_track.track=tracks.id and
								genre_track.genre=genres.id and
								genres.name in ([% excludedgenres %]))
						[%- END %]
						[%- IF excludedcomment %]
						and not exists(select * from comments 
							where 
								comments.track=tracks.id and
								[%- IF containsexcludedcomment %]
								comments.value like '%[% excludedcomment %]%'
								[% ELSE %]
								comments.value='[% excludedcomment %]'
								[%- END %])
						[%- END %]
					group by tracks.id
					order by tracks.disc,tracks.tracknum asc, tracks.titlesort asc
                                </menudata>
