<?xml version="1.0" encoding="utf-8"?>
<custombrowse>
	<requireplugins>TrackStat</requireplugins>
	<template>
		<name>Statistics</name>
		<description>Menu that just put the standard TrackStat menu below the Custom Browse menu</description>
		<parameter type="text" id="menuname" name="Menu name" value="Statistics"/>
		<parameter type="text" id="menuorder" name="Menu sort order (1-100)" value="50"/>
		<parameter type="text" id="statisticgroup" name="Statistic group" value=""/>
		<parameter type="text" id="includedclients" name="Only include on these players (empty = all)" value=""/>
		<parameter type="text" id="excludedclients" name="Exclude from these players" value=""/>
	</template>
</custombrowse>

